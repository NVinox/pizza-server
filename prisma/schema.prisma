generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Category {
  id   Int    @id @default(autoincrement())
  text String @unique
}

model SortCategory {
  id   Int    @id @default(autoincrement())
  text String @unique
}

model Dough {
  id      Int    @id @default(autoincrement())
  caption String @unique
}

model Size {
  id   Int @id @default(autoincrement())
  size Int @unique
}
